extends ../layouts/layout_main.pug
block container
  .page-title
    .title_left
      h3
        | Compromisos 
    .title_right
      .col-md-5.col-sm-5.col-xs-12.form-group.pull-right.top_search
        .input-group
          input.form-control(type='text', placeholder='Search for...')
          span.input-group-btn
            button.btn.btn-default(type='button') Go!
  .clearfix
  .row
    .col-md-12
      .x_panel
        .x_title
          h2 Listado
          ul.nav.navbar-right.panel_toolbox
          .clearfix
        .x_content
          p Simple table with project listing with progress and editing options
          // start project list
          table.table.table-striped.projects
            thead
              tr
                th(style='width: 1%') #
                th(style='width: 20%') Project Name
                th Team Members
                th Project Progress
                th Status
                th(style='width: 20%') #Edit
            tbody
              tr
                td #
                td
                  a Pesamakini Backend UI
                  br
                  small Created 01.01.2015
                td
                  ul.list-inline
                    li
                      img.avatar(src='images/user.png', alt='Avatar')
                    li
                      img.avatar(src='images/user.png', alt='Avatar')
                    li
                      img.avatar(src='images/user.png', alt='Avatar')
                    li
                      img.avatar(src='images/user.png', alt='Avatar')
                td.project_progress
                  .progress.progress_sm
                    .progress-bar.bg-green(role='progressbar', data-transitiongoal='57')
                  small 57% Complete
                td
                  button.btn.btn-success.btn-xs(type='button') Success
                td
                  a.btn.btn-primary.btn-xs(href='#')
                    i.fa.fa-folder
                    |  View
                  a.btn.btn-info.btn-xs(href='#')
                    i.fa.fa-pencil
                    |  Edit
                  a.btn.btn-danger.btn-xs(href='#')
                    i.fa.fa-trash-o
                    |  Delete
